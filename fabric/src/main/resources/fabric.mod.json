{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "${mod_url}",
    "issues": "${mod_issues}",
    "sources": "${mod_sources}"
  },
  "license": "${mod_license}",
  "icon": "${mod_logo}",
  "environment": "*",
  "entrypoints": {
    "main": [
      "mod.azure.doomangelring.FabricLibMod"
    ]
  },
  "mixins": [
    "${mod_id}.fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": "*",
    "java": ">=17",
    "minecraft": "~${minecraft_version}",
    "trinkets": ">=3.7.0",
    "azurelib": ">=2.0.16"
  }
}